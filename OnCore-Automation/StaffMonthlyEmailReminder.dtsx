                      dataType="System.String"><![CDATA[using System.Text;

internal class EmailRecord
{
    public string EmailTo { get; private set; }
    public string Subject { get; private set; }
    public string Body { get; private set; }
    private EmailRecord()
    {
        
    }
    

    public class Builder
    {

        private EmailRecord emailRecord;
        public Builder()
        {
            emailRecord = new EmailRecord();
            emailRecord.Subject = "Monthly Reminder to Update OnCore for Quarterly Report";
        }
        public Builder WithEmailTo(string emailTo)
        {
            emailRecord.EmailTo = emailTo;
            return this;
        }
        public Builder WithBody(string protocols)
        {
            var protocolsList = ExtractProtocols(protocols);
            StringBuilder sb = new StringBuilder();
            sb.Append("<html> " +
        "<head> " +
        "<meta http-equiv=Content-Type content=\"text/html; charset=windows-1252\">" +
        "</head>");

            sb.Append("<body lang=EN-US link=\"#0563C1\" vlink=\"#954F72\">" +
                "<br>" + "This is a monthly reminder to update your " + "<a href=\"https://ctapps.uahs.arizona.edu/oncore/summary-accrual-reporting\">summary accrual data</a> in OnCore in preparation for the quarterly enrollment reports to leadership." +
                 //"<br>" + "Please update:" +
                 "<br>" + "<b>" + "Please update:" + " </b>" +
                 "<li>" + "<b>" + "Subject Accrual*" + " </b>" + "</li>" +
                 "<li>" + "<b>" + "IRB expiration dates" + " </b>" + "</li>" +
                 "<li>" + "<b>" + "Protocol statuses (open to accrual, closed to accrual, etc.)" + " </b>" + "</li>" +
                 "<li>" + "<b>" + "Protocol staff (add or remove staff as needed)" + " </b>" + "</li>" +
                 "<li>" + "<b>" + "Target accruals" + "</b>" + "</li>" +
                 "<br> Protocols ");

            // Append the dynamic list of protocol numbers

            foreach (string protocolNumber in protocolsList)
            {
                sb.Append("<p>" + protocolNumber + "</p>");
            }

            sb.Append("<br>" + "*For those that use the summary accrual method, " + "<a href=\"https://arizona.app.box.com/f/4cf24bef6c0a4e0cb1688f7886385ee0\">please upload</a> an updated accrual file. If there are no updates, no new file is needed. These protocols are identified with (Y) after the protocol number." +
                //"<br>" + bodyws.Cells[11, 1] +
                "<br>" + "If your accrual data is in another format (e.g. sponsor provided accrual reports), " + "<a href=\"https://arizona.app.box.com/f/4cf24bef6c0a4e0cb1688f7886385ee0\">please upload</a> this information and we will assist with converting this information to upload into OnCore." +
                //"<br>" + bodyws.Cells[11, 1] +
                "<br>" + "If you have any questions about this process, please submit a ticket in " + "<a href=\"https://ticketcat.arizona.edu\">TicketCat</a>." +
                //"<br>" + bodyws.Cells[11, 1] +
                "<br>" + "<b>" + "Helpful links:" + " </b>" +
                "<li>" + "<a href=\"https://ctapps.uahs.arizona.edu/oncore/summary-accrual-reporting\">Summary Accrual Info Page</a>" + "</li>" +
                "<li>" + "<a href=\"https://app.box.com/s/4dexex14ze56mmpix6hq2jhdhs8c4tz7\">Summary Accrual Template</a>" + "</li>" +
                "<li>" + "<a href=\"https://arizona.app.box.com/f/4cf24bef6c0a4e0cb1688f7886385ee0\">Summary Accrual Upload Link</a>" + "</li>" +
                "<br>" + "Kind regards," +
                "<br>" + "OnCoreIT");

            sb.Append("</body> " +
                "</html>");

            emailRecord.Body = sb.ToString();
            return this;
        }

        private string[] ExtractProtocols(string protocols)
        {
            string[] result = protocols.Split(',');
            return result;
        }

        public EmailRecord Build()
        {
            return emailRecord;
        }
    }

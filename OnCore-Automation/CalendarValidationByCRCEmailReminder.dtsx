                    <arrayElement
                      dataType="System.String"><![CDATA[using System;
using System.Collections.Generic;
using System.Net.Mail;
using System.Text;

internal class EmailRecord
{
    private HashSet<string> RecipientName;
    private HashSet<string> EmailTo;
    private HashSet<string> EmailCC;
    public MailMessage mailMessage { get; private set; }
    private EmailRecord()
    {
        RecipientName = new HashSet<string>();
        EmailTo = new HashSet<string>();
        EmailCC = new HashSet<string>();
        mailMessage = new MailMessage();
        mailMessage.IsBodyHtml = true;
    }


    public class Builder
    {

        private EmailRecord emailRecord;
        public Input0Buffer Row { get; private set; }

        public Builder()
        {
            emailRecord = new EmailRecord();
            //emailRecord.Subject = "Monthly Reminder to Update OnCore for Quarterly Report";
        }

        public Builder WithEntity(Input0Buffer row)
        {
            this.Row = row;
            return this;
        }
        private void WithRecipientName(string name)
        {
            foreach (var item in ExtractList(name))
            {
                emailRecord.RecipientName.Add(item);
            }

        }
        public Builder WithEmailFrom(string emailFrom)
        {
            emailRecord.mailMessage.From = new MailAddress(emailFrom);
            return this;
        }
        private Builder WithEmailTo(string emailTo)
        {

            foreach (var item in ExtractList(emailTo))
            {
                emailRecord.EmailTo.Add(item);
            }
            foreach (var item in emailRecord.EmailTo)
            {
                emailRecord.mailMessage.To.Add(item);
            }
            return this;
        }

        private Builder WithCC(string emailCC)
        {
            if (emailCC.Length != 0)
            {
                string[] listOfCC = ExtractList(emailCC);
                foreach (var item in listOfCC)
                {
                    emailRecord.EmailCC.Add(item);
                }

                foreach (var item in emailRecord.EmailCC)
                {
                    if (!emailRecord.EmailTo.Contains(item))
                    {
                        emailRecord.mailMessage.CC.Add(item);
                    }
                }
            }
            return this;
        }
        private Builder withBCC(string email)
        {

            emailRecord.mailMessage.Bcc.Add(email);
            return this;
        }

        private Builder WithSubject(string protocolNo)
        {
            emailRecord.mailMessage.Subject = $"{protocolNo}: CRC Signoff Needed in OnCore for Calendar Validation > {Row.EmailType}";
            return this;
        }

        private Builder WithBody()
        {

            string body = $@"
<!DOCTYPE html>
<html lang=""en"">
<head>
<meta charset=""UTF-8"">
<meta http-equiv=Content-Type content=""text/html; charset=windows-1252"">
<style>
  table {{
    width: 100%;
    border-collapse: collapse;
  }}

  th, td {{
    border: 1px solid black;
    padding: 8px;
    text-align: left;
  }}

  th {{
    background-color: #f2f2f2;
  }}
</style>
</head>

<body lang=EN-US link=""#0563C1"" vlink=""#954F72"">
<br>Dear {Row.protocolno} team:
<br>
<br>The study referenced above was sent for validation on {Row.SentForValidationDate}. You should have received an automated validation request email from OnCore Support on that date.
<br>
<br>Please review the calendar as soon as possible. It has been {Row.DaysSinceCalendarCompleted} days since this calendar was sent for validation.
<br>
<br>If there are changes that need to be made to the calendar, or if you have questions, please contact the OnCore team at <a href=""mailto:OnCoreSupport@email.arizona.edu"">OnCoreSupport@email.arizona.edu</a>.
<br>
<br>If the calendar is accurate, please click the Coordinator Signoff button below the calendar in OnCore > Menu > eCRFs/Calendars > Specifications. An automated email will be sent to OnCore Support when that button is clicked.
<br>
<br><u>For future reference, Calendar Validation is required within 5 business days of receiving the automated validation-request email from OnCore.</u>
<br>
<br>@IRB Coordinator -- If the Primary CRC is needed (see table below), please update OnCore accordingly and forward to that person.
<br>
<br>@PI --Please assist in getting this calendar validated.
<br>
<table>
  <tr>
    <th colspan=""2"">Protocol Info</th> <!-- Use colspan to span the header across two columns -->
  </tr>
  <tr>
    <td>Protocol Number</td>
    <td>{Row.protocolno}</td>
  </tr>
  <tr>
    <td>PI Name</td>
    <td>{Row.piname}</td>
  </tr>
  <tr>
    <td>Title</td>
    <td>{Row.Title}</td>
  </tr>
  <tr>
    <td>Sponsor</td>
    <td>{Row.sponsor}</td>
  </tr>
  <tr>
    <td>Sponsor Number</td>
    <td>{Row.sponsorNo}</td>
  </tr>
  <tr>
    <td>Primary CRC</td>
    <td>{Row.PrimaryCRCNames}</td>
  </tr>
  <tr>
    <td>Primary IRB Coordinator</td>
    <td>{Row.PrimaryIRBCName}</td>
  </tr>
</table>
<br>
<br>Please do NOT reply to this email. It's an automated message. 
<br>
<br>Sincerely,
<br>College of Medicine, Information Technology Services
</body>
</html>
";

            emailRecord.mailMessage.Body = body;
            return this;
        }

        private string[] ExtractList(string emailTo)
        {
            return emailTo.Split(';');
        }



        public EmailRecord Build()
        {

            if (!string.IsNullOrEmpty(Row.PrimaryCRCEmails))
            {
                WithEmailTo(Row.PrimaryCRCEmails);
                string cc = !string.IsNullOrEmpty(Row.PrimaryIRBCEmail) ?
                    !string.IsNullOrEmpty(Row.piemail) ?
                    $"{Row.PrimaryIRBCEmail};{Row.piemail}" :
                    $"{Row.PrimaryIRBCEmail}" :
                    !string.IsNullOrEmpty(Row.piemail) ?
                    $"{Row.piemail}" :
                    "";
                WithCC(cc);
            }
            else if (!string.IsNullOrEmpty(Row.PrimaryIRBCEmail))
            {
                WithEmailTo(Row.PrimaryIRBCEmail)
                    .WithCC(Row.piemail);

            }
            else
            {
                WithEmailTo(Row.piemail);
            }

            //WithCC("oncoresupport@arizona.edu")
            //withBCC("gendy@arizona.edu")
            WithBody()
            .WithSubject(Row.protocolno);

            return emailRecord;
        }
    }

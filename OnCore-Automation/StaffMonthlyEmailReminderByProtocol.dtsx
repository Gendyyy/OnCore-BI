                      dataType="System.String"><![CDATA[using System;
using System.Collections.Generic;
using System.Net.Mail;
using System.Text;

internal class EmailRecord
{
    public HashSet<string> RecipientName { get; private set; }
    public HashSet<string> EmailTo { get; private set; }
    public MailMessage mailMessage { get; private set; }
    private EmailRecord()
    {
        RecipientName = new HashSet<string>();
        EmailTo = new HashSet<string>();
        mailMessage = new MailMessage();
        mailMessage.IsBodyHtml = true;
    }


    public class Builder
    {

        private EmailRecord emailRecord;
        public Builder()
        {
            emailRecord = new EmailRecord();
            //emailRecord.Subject = "Monthly Reminder to Update OnCore for Quarterly Report";
        }
        public Builder WithRecipientName(string name)
        {
            foreach (var item in ExtractList(name))
            {
                emailRecord.RecipientName.Add(item);
            }
            
            return this;
        }
        public Builder WithEmailFrom(string emailFrom)
        {
            emailRecord.mailMessage.From = new MailAddress(emailFrom);
            return this;
        }
        public Builder WithEmailTo(string emailTo)
        {

            foreach (var item in ExtractList(emailTo))
            {
                emailRecord.EmailTo.Add(item);
            }
            foreach (var item in emailRecord.EmailTo)
            {
                emailRecord.mailMessage.To.Add(item);
            }
            return this;
        }

        public Builder WithCC(string emailCC)
        {
            if (!string.IsNullOrEmpty(emailCC) && emailCC != "NA")
            {
                emailRecord.mailMessage.CC.Add(emailCC);
            }
            return this;
        }

        public Builder WithSubject(string protocolNo)
        {
            emailRecord.mailMessage.Subject = $"REMINDER: Protocol No. {protocolNo} Action Needed for Summary Accrual Upload";
            return this;
        }

        public Builder WithBody(string protocolNo)
        {
            StringBuilder sb = new StringBuilder();
            sb.Append("<html>");
            sb.Append("<head>");
            sb.Append("<meta http-equiv=Content-Type content=\"text/html; charset=windows-1252\">");
            sb.Append("</head>");
            sb.Append("<body lang=EN-US link=\"#0563C1\" vlink=\"#954F72\">");
            sb.Append($"<br>Dear Clinical Trial Team of Protocol No. <b>{protocolNo}</b>,");
            sb.Append("<br>");

            //foreach (var item in emailRecord.RecipientName)
            //{
            //    string[] firstLastNames = item.Split(',');
            //    if (firstLastNames.Length > 1)
            //    {
            //        sb.Append($"{firstLastNames[1].Trim()}, ");
            //    }
            //    else
            //    {
            //        sb.Append($"{item}, ");
            //    }

            //}
            //sb.Remove(sb.Length - 2, 2);

            sb.Append("<br>Please update the following information in OnCore for <a href = \"https://ctapps.uahs.arizona.edu/oncore/summary-accrual-reporting#:~:text=Summary%20Accrual%20refers%20to%20an,updates%20to%20track%20research%20visits.\">compliance requirements</a> and leadership reports:");
            sb.Append("<ul>");
            sb.Append("<li><b>Subject Accruals</b>");
            sb.Append("<ul>");
            sb.Append("<li>Submit your summary accrual file using <a href= \"https://ticketcat.arizona.edu/support/catalog/items/73\">this request form</a>. If you encounter issues accessing the link in this email, visit <a href=\"https://ticketcat.arizona.edu/\"> TicketCat</a> and submit your file via the 'OnCore Summary Accrual File Upload' request form.</li>");
            sb.Append("<li>If your accrual data is in another format (e.g., sponsor-provided accrual reports), please upload this information using the instructions provided in this email, and we will assist in converting it for OnCore.</li>");
            sb.Append("</ul>");
            sb.Append("</li>");
            sb.Append("<li><b>IRB expiration dates</b></li>");
            sb.Append("<li><b>Protocol Status</b> (open to accrual, closed to accrual, etc.)</li>");
            sb.Append("<li><b>Protocol Staff</b> (add or remove staff as needed)</li>");
            sb.Append("<li><b>Target Accrual</b></li>");
            sb.Append("</ul>");
            //sb.Append($"<br>Protocol : {protocolNo}");
            //sb.Append("<br>");
            //sb.Append("<br>*For those that use the summary accrual method, <a href=\"https://arizona.app.box.com/f/4cf24bef6c0a4e0cb1688f7886385ee0\">please upload</a> an updated accrual file. If there are no updates, no new file is needed. These protocols are identified with (Y) after the protocol number.");
            //sb.Append("<br>If your accrual data is in another format (e.g. sponsor provided accrual reports), <a href=\"https://arizona.app.box.com/f/4cf24bef6c0a4e0cb1688f7886385ee0\">please upload</a> this information, and we will assist with converting this information to upload into OnCore.");
            //sb.Append("<br>If you have any questions about this process, please submit a ticket in <a href=\"https://ticketcat.arizona.edu\">TicketCat</a>.");
            sb.Append("<br>If there are no updates to your protocol, no action is needed.");
            sb.Append("<br>");
            sb.Append("<br><b>Helpful links:</b>");
            sb.Append("<li><a href=\"https://ctapps.uahs.arizona.edu/oncore/summary-accrual-reporting\">Summary Accrual Info Page</a></li>");
            sb.Append("<li><a href=\"https://arizona.box.com/s/yrv09c0zxwe2bp4wvti3ldhu5jif5gln\">Summary Accrual Template</a></li>");
            //sb.Append("<li><a href=\"https://arizona.box.com/s/yrv09c0zxwe2bp4wvti3ldhu5jif5gln">Summary Accrual Upload Link</a></li>");
            sb.Append("<br>");
            sb.Append("<br>Thank you for your prompt attention to this matter.</br>");
            sb.Append("<br>");
            sb.Append("<br>Sincerely,");
            sb.Append("<br>College of Medicine, Information Technology Services");
            sb.Append("<br>");
            sb.Append("<br><i>For any inquiries or assistance, please submit a ticket in</i> <a href=\"https://ticketcat.arizona.edu/\"> TicketCat</a>");
            sb.Append("</body>");
            sb.Append("</html>");

            emailRecord.mailMessage.Body = sb.ToString();
            return this;
        }

        private string[] ExtractList(string emailTo)
        {
            return emailTo.Split(';');
        }

        public EmailRecord Build()
        {
            return emailRecord;
        }
    }
}]]></arrayElement>

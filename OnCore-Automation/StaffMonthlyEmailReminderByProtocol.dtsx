    public override void Input0_ProcessInputRow(Input0Buffer row)
    {
        /*
         * Add your code here
         */

        try
        {
            EmailRecord email = new EmailRecord.Builder()
                .WithRecipientName(row.MembersNames)
                        .WithEmailTo(row.MembersEmails)
                        .WithCC(row.PIEmail)
                        .WithSubject(row.subject)
                        .WithBody(row.ProtocolNo)
                        .Build();
            emailRecords.Add(email);
        }
        catch (Exception e)
        {

            throw new Exception($"Error while building email => {e.Message}");
        }
        
    }


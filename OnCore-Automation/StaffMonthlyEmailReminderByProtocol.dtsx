                      dataType="System.String"><![CDATA[using System;
using System.Collections.Generic;
using System.Text;

internal class EmailRecord
{
    public HashSet<string> RecipientName { get; private set; }
    public HashSet<string> EmailTo { get; private set; }
    public string EmailCC { get; private set; }
    public string Subject { get; private set; }
    public string Body { get; private set; }
    private EmailRecord()
    {
        RecipientName = new HashSet<string>();
        EmailTo = new HashSet<string>();
    }
    

    public class Builder
    {

        private EmailRecord emailRecord;
        public Builder()
        {
            emailRecord = new EmailRecord();
            //emailRecord.Subject = "Monthly Reminder to Update OnCore for Quarterly Report";
        }
        public Builder WithRecipientName(string name)
        {
            foreach (var item in ExtractList(name))
            {
                emailRecord.RecipientName.Add(item);
            }
            return this;
        }
        public Builder WithEmailTo(string emailTo)
        {

            foreach (var item in ExtractList(emailTo))
            {
                emailRecord.EmailTo.Add(item);
            }
            return this;
        }

        public Builder WithCC(string emailCC)
        {
            emailRecord.EmailCC = emailCC;
            return this;
        }

        public Builder WithSubject(string subject)
        {
            emailRecord.Subject = subject;
            return this;
        }

        public Builder WithBody(string protocol)
        {
            StringBuilder sb = new StringBuilder();
            sb.Append("<html>");
            sb.Append("<head>");
            sb.Append("<meta http-equiv=Content-Type content=\"text/html; charset=windows-1252\">");
            sb.Append("</head>");
            sb.Append("<body lang=EN-US link=\"#0563C1\" vlink=\"#954F72\">");
            sb.Append($"<br>Dear ");
            
                foreach (var item in emailRecord.RecipientName)
                {
                    sb.Append($"{item}, ");
                }
            sb.Remove(sb.Length - 2, 2);

            sb.Append("<br>");
            sb.Append("<br>This is a monthly reminder to update your <a href=\"https://ctapps.uahs.arizona.edu/oncore/summary-accrual-reporting\">summary accrual data</a> in OnCore in preparation for the quarterly enrollment reports to leadership.");
            sb.Append("<br>");
            sb.Append("<br><b>Please update:</b>");
            sb.Append("<li><b>Subject Accrual*</b></li>");
            sb.Append("<li><b>IRB expiration dates</b></li>");
            sb.Append("<li><b>Protocol statuses (open to accrual, closed to accrual, etc.)</b></li>");
            sb.Append("<li><b>Protocol staff (add or remove staff as needed)</b></li>");
            sb.Append("<li><b>Target accruals</b></li>");
            sb.Append("<br>");
            sb.Append($"<br>Protocol : {protocol}");
            sb.Append("<br>");
            sb.Append("<br>*For those that use the summary accrual method, <a href=\"https://arizona.app.box.com/f/4cf24bef6c0a4e0cb1688f7886385ee0\">please upload</a> an updated accrual file. If there are no updates, no new file is needed. These protocols are identified with (Y) after the protocol number.");
            sb.Append("<br>If your accrual data is in another format (e.g. sponsor provided accrual reports), <a href=\"https://arizona.app.box.com/f/4cf24bef6c0a4e0cb1688f7886385ee0\">please upload</a> this information, and we will assist with converting this information to upload into OnCore.");
            sb.Append("<br>If you have any questions about this process, please submit a ticket in <a href=\"https://ticketcat.arizona.edu\">TicketCat</a>.");
            sb.Append("<br>");
            sb.Append("<br><b>Helpful links:</b>");
            sb.Append("<li><a href=\"https://ctapps.uahs.arizona.edu/oncore/summary-accrual-reporting\">Summary Accrual Info Page</a></li>");
            sb.Append("<li><a href=\"https://app.box.com/s/4dexex14ze56mmpix6hq2jhdhs8c4tz7\">Summary Accrual Template</a></li>");
            sb.Append("<li><a href=\"https://arizona.app.box.com/f/4cf24bef6c0a4e0cb1688f7886385ee0\">Summary Accrual Upload Link</a></li>");
            sb.Append("<br>Kind regards,");
            sb.Append("<br>OnCoreIT");
            sb.Append("</body>");
            sb.Append("</html>");

            emailRecord.Body = sb.ToString();
            return this;
        }

        private string[] ExtractList(string emailTo)
        {
            return emailTo.Split(';');
        }

        //private string[] ExtractProtocols(string protocols)
        //{
        //    string[] result = protocols.Split(',');
        //    return result;
        //}

        public EmailRecord Build()
        {
            return emailRecord;
        }
    }
}]]></arrayElement>

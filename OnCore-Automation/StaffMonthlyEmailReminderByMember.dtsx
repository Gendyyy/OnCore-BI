                      dataType="System.String"><![CDATA[using System;
using System.Collections.Generic;
using System.Linq;
using System.Net.Mail;
using System.Text;
using System.Threading.Tasks;


internal class EmailService
{
    private MailMessage mailMessage;
    private readonly SmtpClient smtpClient;

    public EmailService()
    {
        
        smtpClient = new SmtpClient("ob1.hc4604-54.iphmx.com");
        smtpClient.EnableSsl = true;
        
    }

    private void Configure()
    {
        
        mailMessage.IsBodyHtml = true;
        mailMessage.From = new MailAddress("VPHS-OnCoreIT@email.arizona.edu");
    }
    private void setTo(string mailTo)
    {
        mailMessage.To.Add(mailTo);
    }
    private void setSubject(string subject)
    {
        mailMessage.Subject = subject;
    }
    private void setBody(string body)
    {
        mailMessage.Body = body;
    }
    public void send(EmailRecord emailRecord)
    {
        mailMessage = new MailMessage();
        Configure();
        setTo(emailRecord.EmailTo);
        setSubject(emailRecord.Subject);
        setBody(emailRecord.Body);
        smtpClient.Send(mailMessage);
    }
}




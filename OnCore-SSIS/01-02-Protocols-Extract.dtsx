p.Accrual_Summary as IsSummaryAccrual,
p.Data_Monitoring,
p.Target_Accrual as LowerTargetAccrual,
p.Target_Accrual_Upper as UpperTargetAccrual,
p.STUDY_TARGET_ACCRUAL as TotalAccrualTarget,
case when sp.investigational_drug = 'Y' and sp.investigational_device = 'Y' then 'Both'
    when sp.investigational_drug = 'Y' then 'Drug'
    when sp.investigational_device = 'Y' then 'Device' end as InvestigationalProduct,
p.Current_Status,
p.Current_Status_Date,
p.Created_Date,
p.Initial_Open_Date,
p.Closed_Date,
p.Study_Closure_Date,
p.primary_crc,
p.primary_irb_coord,
p.sponsor,
p.sponsor_number,
p.phase,
p.[NCT_ID],
p.[SCOPE_DESC]


from ONCOREPROD..UACC_ONCORE_RW_UTILS.PROTOCOLS P
inner join ONCOREPROD..UACC_ONCORE_PROD.SMRS_PROTOCOL sp on p.protocol_id = sp.protocol_id
OUTER APPLY (
    SELECT 
    STRING_AGG(i.institution,';') Institutions,
    count(i.institution) NumberOfInstitutions
    FROM ONCOREPROD..UACC_ONCORE_PROD.RV_PCL_INSTITUTION_BASIC i
    where i.protocol_id = p.protocol_id
    group by i.protocol_id
) Institution
OUTER APPLY (
    SELECT
    STRING_AGG(SS.study_site_name,';') StudySites
    FROM ONCOREPROD..UACC_ONCORE_PROD.RV_PCL_STUDY_SITE SS
    where ss.protocol_id = p.protocol_id
    group by ss.protocol_id
) StudySite
where P.protocol_no is not null</property>

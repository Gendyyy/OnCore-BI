p.protocol_no,
p.Title,
case when charindex(';',p.pi_name) != 0
then left(p.pi_name,charindex(';',p.pi_name)-1)
else p.pi_name end
as pi_name,
p.PI_Email,
p.dmg as ManagementGroup,
p.Protocol_Institutions,
p.Department_Name,
p.[Library],
Institution.Institutions,
Institution.NumberOfInstitutions,

                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT O.Operation_Id                  AS ExecutionID,
       CONVERT(DATETIME, O.start_time) AS ExecutionTime,
       N'Package Error : ' + e.Folder_Name + ' --&gt; ' + p.name                  AS Subject,
       OM.message                      AS Body,
       N'gendy@arizona.edu' as [To],
       N'' as CC,
       N'' as BCC
FROM [SSISDB].[catalog].[operations] O
         INNER JOIN [SSISDB].[catalog].[executions] E ON O.operation_id = E.execution_id
         INNER JOIN [SSISDB].[catalog].[packages] P ON E.package_name = P.name -- Corrected alias
         INNER JOIN [SSISDB].[catalog].[operation_messages] OM ON O.operation_id = OM.operation_id
WHERE e.folder_name = 'OnCore-BI'
  and O.status = 4
  AND OM.message_type = 120
and cast(E.start_time as date)  = cast(getdate() as date)
ORDER BY O.start_time DESC;</property>

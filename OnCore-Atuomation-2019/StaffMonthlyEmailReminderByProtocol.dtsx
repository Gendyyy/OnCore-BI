    {
        /*
         * Add your code here
         */
        
        try
        {
            var rowObj = new RowItem(row.ProtocolNo,
            row.PIEmail,
            row.MembersEmails,
            row.MembersNames,
            row.MembersRoles,
            row.TotalAccrualSnapshot,
            row.ExecutionTime);

            EmailRecord email = new EmailRecord.Builder()
                .WithRow(rowObj)
                .WithRecipientName(row.MembersNames)
                .WithEmailFrom(Variables.SMTPClientFromURL.ToString())
                        .WithEmailTo(row.MembersEmails)
                        .WithCC(row.PIEmail)
                        .WithBCC("gendy@arizona.edu")
                        .WithSubject(row.ProtocolNo)
                        .WithBody(row.ProtocolNo)
                        .Build();
            emailRecords.Add(email);
        }
        catch (Exception e)
        {

            throw new Exception($"Error while building email => {e.Message}");
        }

    }


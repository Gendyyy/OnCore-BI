    private void LogToDb(RowItem row)
    {
        string connectionString = Variables.ConnectionString.ToString();

        using (SqlConnection con = new SqlConnection(connectionString))
        {
            con.Open();
            string values = string.Join(",", new string[] {
            $"'{row.protocolNo}'",
            $"'{row.pIEmail}'",
            $"'{row.membersEmails}'",
            $"'{row.membersNames}'",
            $"'{row.membersRoles}'",
            $"'{row.totalAccrualSnapshot}'",
            $"'{row.executionTime}'"
            });
            string insertStatement = $@"INSERT INTO test.MonthlyReminder (Protocol_No,
                                  PI_Email,
                                  MembersEmails,
                                  MembersNames,
                                  MembersRoles,
                                  TotalAccrualSnapshot,
                                  ExecutionTime)
values ({values})";


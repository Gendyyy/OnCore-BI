        foreach (var email in emailRecords)
        {
            EmailService emailService = new EmailService(connectionString, login, pw);
            emailService.send(email.mailMessage);
            DbContext.LogToDb(email.Row, Variables.DBConnectionString.ToString());
        }
    }

    

    /// <summary>

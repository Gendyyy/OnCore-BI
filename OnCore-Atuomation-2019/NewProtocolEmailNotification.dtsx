                      dataType="System.String"><![CDATA[using System;
using System.Collections.Generic;
using System.Net.Mail;
using System.Text;

internal class EmailRecord
{
    private HashSet<string> RecipientName;
    private HashSet<string> EmailTo;
    private HashSet<string> EmailCC;
    public MailMessage mailMessage { get; private set; }
    private EmailRecord()
    {
        RecipientName = new HashSet<string>();
        EmailTo = new HashSet<string>();
        EmailCC = new HashSet<string>();
        mailMessage = new MailMessage();
        mailMessage.IsBodyHtml = true;
    }


    public class Builder
    {

        private EmailRecord emailRecord;
        public Input0Buffer Row { get; private set; }

        public Builder()
        {
            emailRecord = new EmailRecord();
            //emailRecord.Subject = "Monthly Reminder to Update OnCore for Quarterly Report";
        }

        public Builder WithEntity(Input0Buffer row)
        {
            this.Row = row;
            return this;
        }
        private void WithRecipientName(string name)
        {
            foreach (var item in ExtractList(name))
            {
                emailRecord.RecipientName.Add(item);
            }

        }
        public Builder WithEmailFrom(string emailFrom)
        {
            emailRecord.mailMessage.From = new MailAddress(emailFrom);
            return this;
        }
        private Builder WithEmailTo(string emailTo)
        {

            foreach (var item in ExtractList(emailTo))
            {
                emailRecord.EmailTo.Add(item);
            }
            foreach (var item in emailRecord.EmailTo)
            {
                emailRecord.mailMessage.To.Add(item);
            }
            return this;
        }

        private Builder WithCC(string emailCC)
        {
            emailRecord.mailMessage.CC.Add("oncoresupport@arizona.edu");
            if (emailCC.Length != 0)
            {
                string[] listOfCC = ExtractList(emailCC);
                foreach (var item in listOfCC)
                {
                    emailRecord.EmailCC.Add(item);
                }

                foreach (var item in emailRecord.EmailCC)
                {
                    if (!emailRecord.EmailTo.Contains(item))
                    {
                        emailRecord.mailMessage.CC.Add(item);
                    }
                }
            }
            return this;
        }
        private Builder withBCC(string email)
        {

            emailRecord.mailMessage.Bcc.Add(email);
            return this;
        }

        private Builder WithSubject(string protocolNo)
        {
            emailRecord.mailMessage.Subject = $"{Row.protocolno}:  Newly Entered Protocol into OnCore ({Row.CreatedDate.ToShortDateString()})";
            return this;
        }

        private Builder WithBody()
        {
            string body = $@"<html><HEAD><title>protocolinfo</title>
                      <style> table {{border-collapse: collapse; }}  td {{border: thin solid black ;padding: 4px }} </style>
                      <body>
                      This is an autogenerated message from OnCore.<br><br> 
                      @RegulatoryCoordinator -- This study is now entered in OnCore.  Please review the information that has been entered in by Research Administration and update OnCore as needed on the PC Console.<br><br>
                      This includes all fields on the PC Console->Main->Details tab, the IRB No on the Management tab, Sponsor Info, and of course, adding all of the staff on the Staff tab.<br><br>
                      In addition, please proceed with creating and uploading the IRB information and approved documents.<br><br>
                      @OnCoreSupport -- If the Primary IRB Coordinator field listed below is empty, the coordinator did not receive this email.  Please forward the email to them and update the Primary IRB Coordinator in OnCore.<br><br>
                      <table>
                          <tr><td bgcolor=""D3D3D3"";  colspan=""2""><b>Protocol Info</b></td>
                          <tr><td>Protocol Number</td>
                              <td>{Row.protocolno}</td>
                          </tr>
                          <tr><td>PI Name</td>
                              <td>{Row.piname}</td>
                          </tr>
                          <tr><td>Title</td>
                              <td>{Row.Title}</td>
                          </tr>
                          <tr><td>Sponsor</td>
                              <td>{Row.Sponsor}</td> 
                          </tr>
                          <tr><td>Sponsor Number</td>
                              <td>{Row.SponsorNo}</td>
                          </tr>
                          <tr><td>Primary IRB Coordinator</td>
                              <td>{Row.PrimaryIRBC}</td> 
                          </tr>
                      </table>
                      <br><br>
                      </body>
                      </html>

<br><br>Sincerely,
<br>College of Medicine, Information Technology Services";

            emailRecord.mailMessage.Body = body;
            return this;
        }




        public EmailRecord Build()
        {
            //this.emailRecord.EmailCC.Add("oncoresupport@arizona.edu");

            WithEmailTo(Row.PrimaryIRBCEmail)
            .withBCC("gendy@arizona.edu")
            .WithBody()
            .WithSubject(Row.protocolno);

            return emailRecord;
        }


        private string[] ExtractList(string emailTo)
        {
            return emailTo.Split(';');
        }
    }
}]]></arrayElement>

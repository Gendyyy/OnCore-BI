    public override void PostExecute()
    {
        base.PostExecute();
        /*
         * Add your code here
         */
        EmailService emailService = new EmailService();
        foreach (var emailRecord in emailRecords)
        {
            emailService.send(emailRecord.mailMessage);
        }
    }


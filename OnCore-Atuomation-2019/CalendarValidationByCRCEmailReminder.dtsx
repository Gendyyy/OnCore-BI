         */

        try
        {

            if (ShouldSendEmailRecord(row))
            {
                CreateEmailRecord(row);
            }
        }
        catch (Exception e)
        {

            throw new Exception($"Error while building email => {e.Message}");
        }
        
    }

    private void CreateEmailRecord(Input0Buffer row)
    {
        EmailRecord emailRecord = new EmailRecord.Builder()
                                //.WithRecipientName(row.MembersNames)
                                .WithEmailFrom(Variables.SMTPClientFromURL.ToString())
                                        .WithEntity(row)
                                        .Build();
        emailRecords.Add(emailRecord);
    }
    private bool ShouldSendEmailRecord(Input0Buffer row)
    {
        int[] reminderDays = { 7, 15, 21 };
        int daysSinceCalendarCompleted = row.DaysSinceCalendarCompleted;

        if (reminderDays.Contains(daysSinceCalendarCompleted))
        {
            return true;
        }
        if (daysSinceCalendarCompleted > 21 && DateTime.Today.DayOfWeek.ToString() == "Tuesday")
        {
            return true;
        }
        return false;
    }
